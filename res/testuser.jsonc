// příklad sftp uživatele v JSONC formátu
{
    "users": [
        {
            "sftpuser": "testovaci_test", // systémový uživatel
            "sambaVault" : true, // zda vytvořit mountpointy přes sambu nebo přímo bindem
            "sftpmounts": { // mount pointy pro SFTP uživatele se skutenou cílovou cestou - musí existovat
                "img": "/mnt/img/OrangePi/3b/rw",
                "opis": "/home/dvestezar/pro_opis",
                "bkg": "/var/backups"
            },
            "sftpcerts": [ // seznam SSH klíčů pro přístup k SFTP serveru, buď RAW nebo přidáme na začátek 'b64:' a zakódujeme do base64
                "b64:"
            ],
            "pointsSet":{ // optional, pokud uvedeme na property je název mountpointu jako v sftpmounts
                "img":{ // jsou jen property 'my' a 'perm' a nejsou povinné - toto platí jen pro samaba mounty
                    // "my":true, // (optional default false) pokud je true, tak se tento sftp uživatel nastavuje jako vlastník
                    // jinak je vlastní z daného adresáře                    

                    // "rw":false // default je true, pokud je false, tak je mount readonly
                },
                "opis":{
                    // jen readonly
                    "rw":true
                }
            }
        },
        {
            "sftpuser": "testovaci_test_2", // systémový uživatel
            "sambaVault" : true, // zda vytvořit mountpointy přes sambu nebo přímo bindem
            "sftpmounts": { // mount pointy pro SFTP uživatele se skutenou cílovou cestou - musí existovat
                "img": "/mnt/img/OrangePi/3b/rw",
                "opis": "/home/dvestezar/pro_opis",
                "bkg": "/var/backups"
            },
            "sftpcerts": [ // seznam SSH klíčů pro přístup k SFTP serveru, buď RAW nebo přidáme na začátek 'b64:' a zakódujeme do base64
                "b64:"
            ]
        }
    ]   
}
// sambaVault: (bool) není povinný, default je false
//      pokud je true, vytvoří se mountpointy přes sambu (CIFS), kde je zajištěno správné vlastnictví a oprávnění
//      cílového adresáře mountpointu 